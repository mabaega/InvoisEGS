      "Description": "<div id='cfActions'><script>document.addEventListener('DOMContentLoaded',function(){const updateButton=document.querySelector('button.btn.btn-success[onclick=\"ajaxPost(true)\"]'),isUpdate=!!updateButton,customFields=[{label:'Submission UID',actions:['readonly','clear']},{label:'Integration Type',actions:['readonly','clear']},{label:'Format',actions:['readonly','clear']},{label:'Version',actions:['readonly','clear']},{label:'Submission Date',actions:['readonly','clear']},{label:'Document UUID',actions:['readonly','clear']},{label:'Document Issue Date',actions:['readonly','clear']},{label:'Document LongID',actions:['readonly','clear']},{label:'Submission Status',actions:['readonly','clear']},{label:'Public URL',actions:['readonly','clear']}],disableDateAndReference=()=>{const dateInput=document.querySelector('.mx-input');if(dateInput){dateInput.disabled=!0;const calendarIcon=document.querySelector('.mx-icon-calendar');calendarIcon&&(calendarIcon.style.pointerEvents='none',calendarIcon.style.opacity='0.5')}const checkbox=document.querySelector('.input-group-addon input[type=\"checkbox\"]'),inputText=document.querySelector('.input-group input[type=\"text\"]');checkbox&&inputText&&''!==inputText.value&&(app.AutomaticReference=!1,checkbox.checked=!1,checkbox.disabled=!0,inputText.disabled=!0)},processCustomFields=()=>{!isUpdate&&app?.CustomFields2?.Strings&&Object.keys(app.CustomFields2.Strings).forEach(uuid=>{app.CustomFields2.Strings[uuid]=''}),customFields.forEach(field=>{const customField=Array.from(document.querySelectorAll('.form-group')).find(group=>group.querySelector('label')?.textContent.trim()===field.label);if(!customField)return;const input=customField.querySelector('input, textarea');!isUpdate&&field.actions.includes('clear')&&input&&(input.value=''),field.actions.includes('readonly')&&input&&input.setAttribute('readonly','true'),field.actions.includes('hide')&&(customField.style.display='none')})},generateAutoDate=()=>{if(!isUpdate){const now=new Date,currentDateTime=now.toISOString().slice(0,19).replace('T',' ');app.CustomFields2.Strings['65bf01ec-9d33-45c5-b441-4b29b1bfdfba']=currentDateTime,app.IssueDate=currentDateTime.slice(0,10);const documentIssuedDateField=Array.from(document.querySelectorAll('.form-group')).find(group=>group.querySelector('label')?.textContent.trim()==='Document Issue Date');if(documentIssuedDateField){const input=documentIssuedDateField.querySelector('input');input&&(input.value=currentDateTime,input.setAttribute('readonly','true'))}}},disableUpdateButtonValidStatus=()=>{if(isUpdate&&app?.CustomFields2?.Strings){const statusValue=app.CustomFields2.Strings['fd3bc68f-f177-45a4-805f-f5e477ad8833']?.trim().toLowerCase(),lockedStatus=['submitted','valid','cancelled','rejected'];updateButton.disabled=lockedStatus.includes(statusValue)}};try{disableDateAndReference(),processCustomFields(),generateAutoDate(),disableUpdateButtonValidStatus()}catch(error){}document.getElementById('cfActions')?.remove()});</script></div>",
